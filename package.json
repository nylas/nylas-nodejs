{
  "name": "nylas",
  "version": "4.0.0",
  "description":
    "A NodeJS wrapper for the Nylas REST API for email, contacts, and calendar.",
  "main": "lib/nylas.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint \"./!(lib)/**/*.js\"",
    "prettier":
      "prettier --single-quote --trailing-comma es5 --write \"./!(lib)/**/*.js\"",
    "build": "babel src --presets babel-preset-env --out-dir lib",
    "prepublish": "yarn build"
  },
  "keywords": ["email", "contacts", "calendar", "nylas"],
  "author":
    "Ben Gotow <ben@nylas.com>, Karim Hamidou <karim@nylas.com> (https://nylas.com/)",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "1.0.4",
    "async": "0.9.0",
    "backoff": "2.4.1",
    "bluebird": "^2.9.13",
    "clone": "1.0.1",
    "request": "^2.63.0",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "babel-cli": "6",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jest": "^21.2.1",
    "prettier": "^1.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nylas/nylas-nodejs.git"
  }
}
